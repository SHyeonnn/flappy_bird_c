#include <stdio.h>
#include <windows.h>

void gotoxy(int x, int y) {
   COORD pos = { 0, 18 };
   SetConsoleCursorPosition(GetStdHandle(STD_OUTPUT_HANDLE), pos);
}

void console() {
   system("mode con cols=50 lines=50");
}

void bird() {
   printf("      ***         \n ");
   printf("   *   O *           \n ");
   printf("   *     *     \n");
   printf("      ***        \n");
   printf("      L L    \n");
   printf("         \n");
}

